clc
clear all
close all
format longg

me(1,1,1)
m_e=zeros(25,1)

for i = 1:5
    sig0 = i*100+100
    for j = 1:5
        B = j*.2+.6
        for k = 1:3
            eta = k*.1+.6
            m_e = me(B,sig0,eta)
      
        end
    end
end





function m_e = me(B,sig0,eta)
    N_B = (B-.6)/.2;
    N_sig0 = (sig0-100)/100;
    N_eta = (1-eta)/.1;
    m_e = 1000*1.2^(N_B-1)*1.2^(N_sig0-1)*2^(N_eta-1);
end
